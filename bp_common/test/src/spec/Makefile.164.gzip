.DEFAULT_GOAL = all

SUITE = CINT2000
BENCHMARK = 164.gzip

MAX_CYCLES = 1000000000

BENCHMARK_OBJS = \
	bits.o deflate.o gzip.o getopt.o inflate.o lzw.o \
	spec.o trees.o unlzh.o unlzw.o unpack.o unzip.o util.o zip.o

INPUT_TYPE = smred

BENCHMARK_INPS = $(INPUT_TYPE).random

$(BENCHMARK_INPS): %:
	cp $(BENCHMARK_DIR)/input/$@ $@

BENCHMARK_DEFS = -D__raw__ -DSPEC_CPU2000

ARGC=3
ARGV=gzip $(BENCHMARK_INPS) 8

# lfs disk image size
LFS_BLOCK_SIZE = 128
LFS_BLOCK_COUNT = 70000
#LFS_BLOCK_COUNT = 1600
#LFS_BLOCK_COUNT = 50000
#LFS_BLOCK_COUNT = 100000
#LFS_BLOCK_COUNT = 8192 # 1MB


include ./Makefile.common
